{
  "AWSEBDockerrunVersion": 2,
  "volumes": [
    {
      "name": "rxpowet-app",
      "host": {
        "sourcePath": "/app/"
      }
    },
    {
      "name": "nginx-proxy-conf",
      "host": {
        "sourcePath": "/var/app/current/proxy/conf.d"
      }
    }
  ],
  "containerDefinitions": [
    {
      "name": "rxpowet",
      "essential": true,
      "image": "ACCOUNTID.dkr.ecr.us-east-1.amazonaws.com/spider-walker/emrys:0.0.4",
      "memory": 512,
      "mountPoints": [
        {
          "sourceVolume": "rxpowet-app",
          "containerPath": "/app/",
          "readOnly": true
        }
      ]
    },
    {
      "name": "nginx-proxy",
      "image": "nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "rxpowet"
      ],
      "mountPoints": [
        {
          "sourceVolume": "rxpowet-app",
          "containerPath": "/app",
          "readOnly": true
        },
        {
          "sourceVolume": "nginx-proxy-conf",
          "containerPath": "/etc/nginx/conf.d",
          "readOnly": true
        }
      ]
    }
  ]
}