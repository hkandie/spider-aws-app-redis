{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "rxpowet",
      "essential": true,
      "image": "ACCOUNTID.dkr.ecr.us-east-1.amazonaws.com/spider-walker/emrys:0.0.1",
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 9000,
          "containerPort": 9000
        }
      ]
    },
    {
      "name": "nginx-proxy",
      "image": "nginx",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "rxpowet"
      ]
    }
  ]
}